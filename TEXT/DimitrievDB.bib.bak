@Article{A2_01_Zutic_2004_Rev.Mod.Phys.76323-4102004,
  author       = {I. Zutic and J. Fabian and S. D. Sarma},
  title        = {Spintronics: Fundamentals and applications},
  journal      = {Reviews of Modern Physics},
  year         = {2004},
  volume       = {76},
  pages        = {323--410},
  abstract     = {Spintronics, or spin electronics, involves the study of active control and manipulation of spin degrees of freedom in solid-state systems. This article reviews the current status of this subject, including both recent advances and well-established results. The primary focus is on the basic physical principles underlying the generation of carrier spin polarization, spin dynamics, and spin-polarized transport in semiconductors and metals. Spin transport differs from charge transport in that spin is a nonconserved quantity in solids due to spin-orbit and hyperfine coupling. The authors discuss in detail spin decoherence mechanisms in metals and semiconductors. Various theories of spin injection and spin-polarized transport are applied to hybrid structures relevant to spin-based devices and fundamental studies of materials properties. Experimental work is reviewed with the emphasis on projected applications, in which external electric and magnetic fields and illumination by light will be used to control spin and charge dynamics to create new functionalities not feasible or ineffective with conventional electronics.},
  date         = {2004-05-21},
  doi          = {10.1103/RevModPhys.76.323},
  eprint       = {cond-mat/0405528v1},
  eprintclass  = {cond-mat.other},
  eprinttype   = {arXiv},
  file         = {online:http\://arxiv.org/pdf/cond-mat/0405528v1:PDF},
  journaltitle = {Rev. Mod. Phys. 76, 323-410 (2004)},
  keywords     = {cond-mat.other},
}

